      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^13.7.0",
    "npm": ">= 6.13.6",
    "yarn": ">= 1.22.0"
  }
